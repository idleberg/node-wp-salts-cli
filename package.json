{
  "name": "wp-salts-cli",
  "version": "0.6.0",
  "description": "CLI tool to generate WordPress salts in various formats",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project .",
    "test": "npx ava --verbose"
  },
  "bin": {
    "wp-salts": "./bin/wp-salts.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-wp-salts-cli.git"
  },
  "keywords": [
    "wordpress",
    "salts",
    "wordpress-salts"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "sort-keys": "^3.0.0",
    "table": "^5.4.1",
    "wp-salts": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.6.2",
    "ava": "^2.2.0",
    "husky": "^3.0.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  }
}
